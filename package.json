{
  "name": "teved",
  "version": "0.0.1",
  "main": "main.js",
  "license": "WTFPL",
  "scripts": {
    "hot-server": "webpack-dev-server --watch --config webpack.config.dev.js --progress --colors --display-error-details --content-base build --host 0.0.0.0 --port 3000 --hot",
    "start-hot": "better-npm-run start-hot",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "start-hot": {
      "command": "electron .",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "0.0.5",
    "electron-debug": "^0.5.1",
    "font-awesome": "^4.5.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.0",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-target-electron-renderer": "^0.2.0"
  }
}
